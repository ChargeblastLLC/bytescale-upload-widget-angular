{
  "name": "angular-uploader-workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "ng test --watch=false --browsers ChromeHeadless",
    "test:e2e": "npx wdio run tests/wdio.conf.ts --autoCompileOpts.tsNodeOpts.project=tests/tsconfig.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*/**/*.{js,jsx,ts,tsx,test}": [
      "bash -c \"npm run test:unit\""
    ],
    "*/**/*.{ts,tsx}": [
      "bash -c \"tsc --noEmit\""
    ],
    "*.{js,jsx,ts,tsx,json,css,html,md,yaml,yml}": [
      "prettier -c"
    ]
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uploader": "^3.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@wdio/cli": "^7.19.3",
    "@wdio/local-runner": "^7.19.3",
    "@wdio/jasmine-framework": "^7.19.3",
    "@wdio/spec-reporter": "^7.19.1",
    "chromedriver": "^107.0.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "10.5.1",
    "ng-packagr": "^13.0.0",
    "husky": "^7.0.4",
    "prettier": "2.1.2",
    "ts-node": "^10.7.0",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-wait-for": "^2.2.5",
    "webdriverio": "^7.19.3",
    "typescript": "~4.6.2"
  }
}
